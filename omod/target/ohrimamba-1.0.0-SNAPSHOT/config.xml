<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

    <!-- Base Module Properties -->
    <id>ohrimamba</id>
    <name>OHRIMamba</name>
    <version>1.0.0-SNAPSHOT</version>
    <package>org.openmrs.module.ohrimamba</package>
    <author>Arthur D. Mugume [OHRI]</author>
    <description>OHRI Mamba OpenMRS module</description>

    <activator>org.openmrs.module.ohrimamba.OHRIMambaActivator</activator>

    <require_version>2.3.1</require_version>

    <aware_of_modules>
        <aware_of_module>org.openmrs.module.legacyui</aware_of_module>
    </aware_of_modules>

    <!-- Internationalization -->
    <!-- All message codes should start with ohrimamba.* -->
    <messages>
        <lang>en</lang>
        <file>messages.properties</file>
    </messages>
    <messages>
        <lang>fr</lang>
        <file>messages_fr.properties</file>
    </messages>
    <messages>
        <lang>es</lang>
        <file>messages_es.properties</file>
    </messages>

</module>

